{
  "title": "title",
  "author": "",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "github-buttons",
    "editlink",
    "ga",
    "-highlight",
    "prism",
    "bibtex-indexed-cite",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },

    "github-buttons": {
      "buttons": [{
        "user": "hoppersroppers",
        "repo": "fundamentals",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "editlink": {
      "base": "https://github.com/hoppersroppers/fundamentals/blob/master",
      "label": "Edit",
      "multilingual": false
    },
    "ga": {
      "token": "UA-151638088-2"
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "Roppers Academy 2021",
      "markdown": true,
      "update_label": "Dateï¼š",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "bibtex-indexed-cite": {
      "path": "/"
    }

  }
}
